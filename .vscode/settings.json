{
    "eslint.enable": false,
    "prettier.enable": false,
    "editor.codeActionsOnSave": {
        "quickfix.rome": true,
        "source.organizeImports.rome": false,
        "source.fixAll": false,
        "source.organizeImports": true
    },
    "[javascript]": {
        "editor.defaultFormatter": "rome.rome",
    },
    "[typescript]": {
        "editor.defaultFormatter": "rome.rome",
    },
    "[json]": {
        "editor.defaultFormatter": "rome.rome"
    },
    "cSpell.words": [
        "datetime"
    ]
}